        // searchBtn.addEventListener("click", function(){
        //     searchModal.classList.toggle("active");
        // });
        // clsSearchModal.addEventListener("click", function(){
        //     searchModal.classList.remove("active");
        // });

        var hamburgerBtn = document.querySelector(".hamburger-menu .btn");
        var responsiveMenu = document.querySelector(".responsive-content");

        hamburgerBtn.addEventListener("click", function(){
            responsiveMenu.classList.toggle("active");
            
        })



        var coll = document.getElementsByClassName("responsive-nav");
        var i;

        for (i = 0; i < coll.length; i++) {
        coll[i].addEventListener("click", function() {
            this.classList.toggle("active-border");
            this.classList.toggle("active");
            var content = this.nextElementSibling;
            if (content.style.maxHeight){
            content.style.maxHeight = null;
            } else {
            content.style.maxHeight = content.scrollHeight + "px";
            } 
        });
        }

        // var coll = document.getElementsByClassName("submenu-btn");
        // var i;

        // for (i = 0; i < coll.length; i++) {
